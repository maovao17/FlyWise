{
  "functions": {
    "backend/server.js": {
      "memory": 1024
    },
    "mlService/app.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/predict-fare",
      "destination": "/mlService/app.py"
    },
    {
      "source": "/api/:path*",
      "destination": "/backend/server.js"
    }
  ]
}